psql -U postgres -c "\c ifs4205" -c "\copy (SELECT (date_part('year', current_date) - extract(year from dateofbirth)) as age, round(height) as height, round(weight) as weight, allergies, race, zipcode, sex, diagnosis_id as diagnosis FROM backend_healthrecord h INNER JOIN backend_examination e ON h.user_id = e.patient_id) TO './patient.csv' WITH (DELIMITER ',',FORMAT CSV, HEADER)"